#WAP to find maximum element in a list
l=[]
size=int(input("Enter the size of list\n"))
for i in range(0,size,1):
    data=int(input("Enter the element"))
    l.append(data)
maxx=l[0]
for i in range(0,size,1):
    if l[i]>maxx:
        temp=maxx
        maxx=l[i]
        l[i]=maxx
print("maximum element in list is:",maxx)
